compile: main.c agenda/lista_agenda/lista_agenda.c agenda/tipo_agenda/tipo_agenda.c professor/lista_professor/lista_professor.c professor/tipo_professor/tipo_professor.c compromisso/lista_compromisso/lista_compromisso.c compromisso/tipo_compromisso/tipo_compromisso.c contato/lista_contato/lista_contato.c contato/tipo_contato/tipo_contato.c utils/utils.c
	gcc main.c agenda/lista_agenda/lista_agenda.c agenda/tipo_agenda/tipo_agenda.c professor/lista_professor/lista_professor.c professor/tipo_professor/tipo_professor.c compromisso/lista_compromisso/lista_compromisso.c compromisso/tipo_compromisso/tipo_compromisso.c contato/lista_contato/lista_contato.c contato/tipo_contato/tipo_contato.c  utils/utils.c -o main -lm

